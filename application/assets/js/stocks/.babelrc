{
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 3,
            "targets": {
              "browsers": [
                ">2%",
                "not dead",
                "ie >= 11"
              ]
            }
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
          "babel-plugin-styled-components",
          {
            "displayName": false,
            "ssr": false,
            "pure": true,
            "minify": true
          }
        ]
      ]
    },
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 3,
            "targets": {
              "browsers": [
                ">2%",
                "not dead",
                "ie >= 11"
              ]
            }
          }
        ],
        [
          "@babel/preset-react",
          {
            "targets": {
              "node": 'current'
            }
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-jsx",
        [
          "babel-plugin-styled-components",
          {
            "displayName": true,
            "ssr": false,
            "pure": false,
            "minify": false
          }
        ],
        [
          "contracts",
          {
            "names": {
              "assert": "assert",
              "precondition": "pre",
              "postcondition": "post",
              "invariant": "invariant",
              "return": "it",
              "old": "old"
            }
          }
        ]
      ]
    }
  }
}
